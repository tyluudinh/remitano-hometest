    "class-validator": "^0.14.0",
    "cp-cli": "^2.0.0",
    "env-var": "^7.3.0",
    "mongoose": "^7.0.4",
    "nestjs-console": "^8.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
